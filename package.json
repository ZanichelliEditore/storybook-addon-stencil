{
  "name": "storybook-addon-stencil",
  "version": "0.2.0",
  "description": "A Stencil compiler integration for Storybook.",
  "keywords": [
    "storybook",
    "storybook-addons",
    "stencil",
    "web-components"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ZanichelliEditore/storybook-addon-stencil"
  },
  "author": "Zanichelli Editore",
  "license": "MIT",
  "main": "dist/cjs/client/index.js",
  "module": "dist/esm/client/index.js",
  "types": "dist/ts/client/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "*.js",
    "*.d.ts"
  ],
  "scripts": {
    "prepare": "yarn build",
    "clean": "rimraf ./dist",
    "buildBabel": "concurrently \"yarn buildBabel:cjs\" \"yarn buildBabel:esm\"",
    "buildBabel:cjs": "babel ./src -d ./dist/cjs --extensions \".js,.jsx,.ts,.tsx\"",
    "buildBabel:esm": "babel ./src -d ./dist/esm --env-name esm --extensions \".js,.jsx,.ts,.tsx\"",
    "buildTsc": "tsc --declaration --emitDeclarationOnly --outDir ./dist/ts",
    "prebuild": "yarn clean",
    "build": "concurrently \"yarn buildBabel\" \"yarn buildTsc\"",
    "build:watch": "concurrently \"yarn buildBabel:esm -- --watch\" \"yarn buildTsc -- --watch\"",
    "test": "jest",
    "storybook": "start-storybook -p 6006",
    "start": "concurrently \"yarn build:watch\" \"yarn storybook -- --no-manager-cache --quiet\"",
    "build-storybook": "build-storybook",
    "prerelease": "zx scripts/prepublish-checks.mjs",
    "release": "yarn build && auto shipit",
    "eject-ts": "zx scripts/eject-typescript.mjs"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "@babel/preset-typescript": "^7.13.0",
    "@stencil/core": "^4.3.0",
    "@storybook/addon-essentials": "^7.4.0",
    "@storybook/react": "^7.4.0",
    "auto": "^11.0.4",
    "babel-loader": "^9.1.3",
    "boxen": "^7.1.1",
    "concurrently": "^8.2.1",
    "dedent": "^1.5.1",
    "jest": "^29.3.1",
    "memfs": "^4.3.0",
    "prettier": "^3.0.3",
    "prop-types": "^15.7.2",
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0",
    "rimraf": "^5.0.1",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "zx": "^7.2.3"
  },
  "peerDependencies": {
    "@stencil/core": "^4.3.0",
    "@storybook/addons": "^7.4.0",
    "@storybook/api": "^7.4.0",
    "@storybook/components": "^7.4.0",
    "@storybook/core-events": "^7.4.0",
    "@storybook/theming": "^7.4.0",
    "react": ">=16.0.0",
    "react-dom": ">=16.8.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "storybook": {
    "displayName": "Stencil",
    "supportedFrameworks": [
      "web-components"
    ],
    "icon": "https://raw.githubusercontent.com/ionic-team/stencil/main/stencil-logo.png"
  },
  "dependencies": {
    "custom-elements-manifest": "^2.0.0",
    "loader-utils": "^3.2.1"
  }
}
